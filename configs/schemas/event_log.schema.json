{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EventLog",
  "type": "object",
  "properties": {
    "case_id": { "type": "string" },
    "activity": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" },
    "amount": { "type": "number" },
    "quality": { "type": ["number", "null"] }
  },
  "required": ["case_id", "activity", "timestamp"]
}


